{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://eodash.github.io/eodash-schemas/catalog/eodashboard-collection-schema.json",
    "title": "Eodashboard, RACE Collection",
    "format": "categories",
    "basicCategoryTitle": "General",
    "allOf": [
        {
            "$ref": "https://eodash.github.io/eodash-schemas/catalog/collection-schema.json"
        },
        {
            "type": "object",
            "properties": {
                "EodashIdentifier": {
                    "type": "string",
                    "minLength": 1,
                    "pattern": "^[a-zA-Z0-9_-]+$",
                    "description": "Identifier that will be used in eodashboard or RACE instance"
                }
            },
            "required": ["EodashIdentifier"]
        }
    ]
}
