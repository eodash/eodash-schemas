{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": {
        "type": "object",
        "properties": {
            "Url": {
                "type": "string",
                "description": "URL of the resource"
            },
            "Description": {
                "type": "string",
                "description": "Shorter description about the resource"
            },
            "Logo": {
                "type": "string",
                "description": "URL to a small logo image"
            }
        },
        "additionalProperties": false,
        "required": ["Url", "Description"]
    }
}
